@model IEnumerable<IdentityRole>;

 @{
        Html.RenderPartial("_AddRole", new RoleFormViewModel());
  }
<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">Name</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    @foreach(var role in Model){
        <tr>
        <td>@role.Id</td>
        <td>@role.Name</td>
        <td><a class="btn btn-primary" asp-action="ManagePermissions" asp-route-roleId="@role.Id">Manage Permissions</a></td>
        </tr>
    }
</tbody>
@section Scripts {
  <partial name="_ValidationScriptsPartial" />
}